function sum(values) do
  integer n := 0
	integer total := 0

	while(values[n] != -1) do
		total := total + values[n]
		n := n + 1
	end

	return total
end

integer nextVal := 0
integer inputVals := []
integer index := 0

while(nextVal != -1) do
	input nextVal
	inputVals[index] := nextVal
	index := index + 1
end

integer out := sum(inputVals)