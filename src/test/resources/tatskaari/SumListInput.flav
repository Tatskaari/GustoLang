function sum(values : integer list) -> integer do
    val n : integer := 0
    val total : integer := 0

	while(values[n] != -1) do
		total := total + values[n]
		n := n + 1
	end

	return total
end

val nextVal : integer := 0
val inputVals : integer := []
val index : integer := 0

while(nextVal != -1) do
	input nextVal
	inputVals[index] := nextVal
	index := index + 1
end

val out : integer := sum(inputVals)