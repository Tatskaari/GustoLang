function add(a : number, b : number) : integer do
  return a + b
end

(* This is a comment that has
no bearing on the code *)

function mul(a : number, b : number) : integer do
  return a * b
end

function reduce(start : number, List : integer list, reducer : (integer, integer) -> integer) : integer do
  val total : integer := start

  val n : integer := 0
  val next : integer := List[n]

  while(next != -1) do
    total := reducer(total, next)
    n := n + 1
    next := List[n]
  end

  return total
end

val List : integer list := [1, 2, 3, 4, -1]
val addRes : integer := reduce(0, List, add)
val mulRes : integer := reduce(1, List, mul)
