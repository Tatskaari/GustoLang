function fib(pN, pLast, pLastLast) do
    if pN = 1 then
      return pLast + pLastLast
    end
    integer new := pLast + pLastLast
    integer last := pLastLast
    integer lastLast := new
    integer n := pN - 1
    return fib(n, last, lastLast)
end

function getNthFib(pN) do
  return fib(pN - 2, 1, 1)
end

integer out := getNthFib(7)

